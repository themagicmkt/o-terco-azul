{
    "compilerOptions": {
      "target": "ESNext",
      "module": "ESNext",
      "moduleResolution": "Node",
      "strict": false,
      "jsx": "react-jsx",
      "esModuleInterop": true,
      "allowSyntheticDefaultImports": true,
      "skipLibCheck": true,
      "baseUrl": "./src",
      "paths": {
        "@/*": ["*"]
      },
      "types": ["vite/client", "node"]
    },
    "include": ["src"]
  }